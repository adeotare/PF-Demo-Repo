<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ccpe_r__Utilization_rate__c</fullName>
    <deprecated>false</deprecated>
    <externalId>false</externalId>
    <formula>(ccpe_r__Billable_Hours__c +  ccpe_r__Exempt_Hours__c ) /  ccpe_r__Working_Hours__c</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Denotes the utilization rate of a user for a particular week or month (NOTE this field should not be used for reporting across multiple utilization periods)</inlineHelpText>
    <label>Utilization Rate</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
