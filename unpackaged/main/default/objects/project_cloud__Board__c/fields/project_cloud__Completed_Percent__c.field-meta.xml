<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>project_cloud__Completed_Percent__c</fullName>
    <deprecated>false</deprecated>
    <externalId>false</externalId>
    <formula>IF(project_cloud__Card_Count__c == project_cloud__On_Hold_Card_Count__c, 0, project_cloud__Total_Card_Weight__c / ((project_cloud__Card_Count__c - project_cloud__On_Hold_Card_Count__c) * 100))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Completion Percentage of this Board. Calculated via Card Completion.</inlineHelpText>
    <label>Completion (%)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
