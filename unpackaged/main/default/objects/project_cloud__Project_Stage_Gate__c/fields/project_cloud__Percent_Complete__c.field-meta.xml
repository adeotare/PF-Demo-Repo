<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>project_cloud__Percent_Complete__c</fullName>
    <deprecated>false</deprecated>
    <description>The completion percent of the Stage Gate, calculated automatically by the Stage Gate Checklist Items status</description>
    <externalId>false</externalId>
    <formula>IF(
	((project_cloud__Total_Completed_Items__c &gt; 0) &amp;&amp; ((project_cloud__Total_Checklist_Items__c + project_cloud__Stage_Gate_Process_Step__r.project_cloud__Total_Checklist_Items__c) &gt; 0)),
	IF(
		NOT(ISBLANK(project_cloud__Stage_Gate_Process_Step__c)),
			project_cloud__Total_Completed_Items__c / (project_cloud__Total_Checklist_Items__c + project_cloud__Stage_Gate_Process_Step__r.project_cloud__Total_Checklist_Items__c),
			project_cloud__Total_Completed_Items__c / project_cloud__Total_Checklist_Items__c
	),
	0
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Percent Complete</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
