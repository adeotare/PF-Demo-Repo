<?xml version="1.0" encoding="UTF-8"?>
<MatchingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <matchingRules>
        <fullName>Duplicate_Leads</fullName>
        <label>Duplicate Leads</label>
        <matchingRuleItems>
